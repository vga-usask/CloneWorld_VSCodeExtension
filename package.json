{
	"name": "clone-world",
	"displayName": "CloneWorld",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.36.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:cloneWorld.generateReport",
		"onCommand:cloneWorld.setReportPath",
		"onCommand:cloneWorld.showReportPath",
		"onCommand:cloneWorld.showReport",
		"onCommand:cloneWorld.findClonesInSameClass"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "cloneWorld.generateReport",
				"title": "Generate Report"
			},
			{
				"command": "cloneWorld.setReportPath",
				"title": "Set Report Path"
			},
			{
				"command": "cloneWorld.showReportPath",
				"title": "Show Report Path"
			},
			{
				"command": "cloneWorld.showReport",
				"title": "Show Report"
			},
			{
				"command": "cloneWorld.findClonesInSameClass",
				"title": "Find Clones in Same Class"
			}
		],
		"menus": {
			"editor/context": [
				{
					"command": "cloneWorld.findClonesInSameClass",
					"group": "navigation"
				}
			]
		},
		"configuration": {
			"title": "Clone World",
			"properties": {
				"nicad.path": {
					"type": "string",
					"default": null,
					"description": "Specifies the folder path of NiCad to use."
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.36.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.0.0-next.0"
	},
	"dependencies": {
		"fs": "0.0.1-security",
		"path": "^0.12.7"
	}
}